{
    "name": "POC-cytomine-front-end",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "translations-csv2json": "i18n-csv2json-cli --from src/locales/translations.csv --to src/locales/json --format",
        "serve": "npm run translations-csv2json && vue-cli-service serve",
        "build": "npm run translations-csv2json && vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "buefy": "^0.6.6",
        "bulma": "^0.7.1",
        "cytomine-client": "0.0.1",
        "font-awesome": "^4.7.0",
        "i18n-csv2json-cli": "^1.0.2",
        "lodash": "^4.17.10",
        "vue": "^2.5.16",
        "vue-i18n": "^7.8.1",
        "vue-notification": "^1.3.10",
        "vue-router": "^3.0.1",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.0.0-beta.15",
        "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
        "@vue/cli-service": "^3.0.0-beta.15",
        "vue-template-compiler": "^2.5.16"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
