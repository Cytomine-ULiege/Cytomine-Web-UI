    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.1.1",
        "@vue/cli-plugin-eslint": "^3.1.5",
        "@vue/cli-service": "^3.1.4",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "i18n-csv2json-cli": "^1.0.2",
        "node-sass": "^4.11.0",
        "onchange": "^5.1.3",
        "sass-loader": "^7.1.0",
        "vue-template-compiler": "^2.5.17"
    },
    "postcss": {
        "plugins": {
